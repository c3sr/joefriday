// stats.fbs
namespace structs;

table Stats {
    ClkTck:short;
    Timestamp:long;
    Ctxt:long;
    BTime:long;
    Processes:long;
    CPUs:[CPU];
}

table CPU {
    ID:string;
    User:long;
    Nice:long;
    System:long;
    Idle:long;
    IOWait:long;
    IRQ:long;
    SoftIRQ:long;
    Steal:long;
    Quest:long;
    QuestNice:long;
}

root_type Stats;
