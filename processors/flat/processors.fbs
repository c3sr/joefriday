// processors.fbs
namespace structs;

table Processors {
	Timestamp:long;
	CPUs:long;
	Sockets:int;
	CoresPerSocket:short;
	Socket:[Processor];
}

table Processor {
	PhysicalID:int;
	VendorID:string;
	CPUFamily:string;
	Model:string;
	ModelName:string;
	Stepping:string;
	Microcode:string;
	MHzMin:float;
	MHzMax:float;
	Cache:[CacheInf];
	CPUCores:int;
	ThreadsPerCore:byte;
	BogoMIPS:float;
	Flags:[string];
}

table CacheInf {
	ID:string;
	Size:string;
}

root_type Processors;
