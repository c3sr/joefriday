// processors.fbs
namespace structs;

table Processors {
	Timestamp:long;
	Sockets:int;
	Socket:[Processor];
}

table Processor {
	PhysicalID:int;
	VendorID:string;
	CPUFamily:string;
	Model:string;
	ModelName:string;
	Stepping:string;
	Microcode:string;
	MHzMin:float;
	MHzMax:float;
	CacheSize:string;
	CPUCores:int;
	BogoMIPS:float;
	Flags:[string];
}

root_type Processors;
