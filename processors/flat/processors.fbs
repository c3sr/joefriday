// processors.fbs
namespace structs;

table Processors {
	Timestamp:long;
	Count:short;
	CPU:[CPU];
}

table CPU {
	PhysicalID:short;
	VendorID:string;
	CPUFamily:string;
	Model:string;
	ModelName:string;
	Stepping:string;
	Microcode:string;
	CPUMHz:float;
	CacheSize:string;
	CPUCores:short;
	BogoMIPS:float;
	Flags:[string];
}

root_type Processors;
